@incollection{bemilli_partie_2014,
  title = {{Une partie de chasse \`a l'Aurochs il y a 10 000 ans : le Locus 28704 d'Alizay (Eure, France)}},
  shorttitle = {{Une partie de chasse \`a l'Aurochs il y a 10 000 ans}},
  booktitle = {{Actes des congr\`es nationaux des soci\'et\'es historiques et scientifiques}},
  author = {B{\'e}milli, C{\'e}line and Biard, Miguel and Chauss{\'e}, Christine and Donnart, Klet},
  year = {2014},
  volume = {138},
  pages = {170--187},
  publisher = {{Ed. du CTHS}},
  address = {{Nantes}},
  abstract = {Une fouille arch\'eologique de 12 ha r\'ealis\'ee dans la carri\`ere Lafarge \`a Alizay (Eure) sous la direction de C. Marcigny a permis la mise au jour en 2010 de trois locus attribuables \`a l'extr\^eme fin du Pal\'eolithique sup\'erieur dat\'es entre 9960 {$\pm$} 40 BP et 10100 {$\pm$} 40 BP. Cette contribution porte exclusivement sur les r\'esultats pr\'eliminaires de la fouille d'un des locus r\'ealis\'ee par M. Biard en 2012. Les travaux sur le locus 28704 ont r\'ev\'el\'e une occupation particuli\`erement bien structur\'ee, compos\'ee de restes lithiques et fauniques bien conserv\'es. Aux Aurochs largement dominants est associ\'e un unique bois de chute de Renne dont la pr\'esence soul\`eve certaines questions. L'organisation spatiale exceptionnelle des vestiges, la pr\'esence de plusieurs animaux indiquent clairement une occupation de courte dur\'ee orient\'ee sur l'acquisition et le traitement du gibier. L'\'eventail des contraintes techniques g\'en\'er\'ees par cette activit\'e est abord\'e et contribue \`a la connaissance des modes de vie des Belloisiens.},
  copyright = {free},
  langid = {french}
}

